{
    "authors": [
        {
            "email": "fkooman@tuxed.net",
            "name": "Fran\u00e7ois Kooman"
        }
    ],
    "autoload": {
        "files": [
            "src/sodium_compat.php"
        ],
        "psr-4": {
            "fkooman\\Jwt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "fkooman\\Jwt\\Tests\\": "tests/"
        }
    },
    "description": "Simple and Secure JWT Signer/Verifier",
    "license": "MIT",
    "name": "fkooman/jwt",
    "require": {
        "ext-date": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-spl": "*",
        "paragonie/constant_time_encoding": "^1.0.3|^2.2.0",
        "paragonie/random_compat": ">=1",
        "php": ">= 5.4.8",
        "symfony/polyfill-php56": "^1",
        "symfony/polyfill-php70": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4|^5|^6|^7"
    },
    "suggest": {
        "ext-libsodium": "PHP < 7.2 sodium implementation",
        "ext-sodium": "PHP >= 7.2 sodium implementation"
    },
    "support": {
        "email": "fkooman@tuxed.net",
        "source": "https://git.tuxed.net/fkooman/php-jwt"
    },
    "type": "library"
}
